{
	"manifest_version": 2,
	"name": "TabSave",
	"version": "1.0",
	"description": "Saves open tabs to file and opens file of tabs to browser",
	"icons": {
		"48": "icons/tab-48.png"
	},
	"browser_action": {
		"default_icon": "icons/tab-48.png",
		"default_title": "TabSave",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	]
}